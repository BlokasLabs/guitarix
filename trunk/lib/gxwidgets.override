#include <Python.h>
#define PY_ARRAY_UNIQUE_SYMBOL gxwidgets_ARRAY_API
#define NO_IMPORT_ARRAY
#include <numpy/ndarrayobject.h>
#include "pygobject.h"
#include "GtkGxTuner.h"
#include "GtkGxFastMeter.h"
