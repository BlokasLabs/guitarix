@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Hermann Meyer" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gxtubedelay>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "GxRedeyeFx" .

<http://guitarix.sourceforge.net/plugins/gxtubedelay#tubedelay>
    a lv2:Plugin ,
        lv2:DelayPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxTubeDelay";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gxtubedelay> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    
  guiext:ui <http://guitarix.sourceforge.net/plugins/gxtubedelay#gui>;
  
    lv2:minorVersion 28;
    lv2:microVersion 3;
    
rdfs:comment """

...

""";

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "fslider0_" ;
        lv2:name "gain";
        lv2:default -40.0 ;
        lv2:minimum  -40.0;
        lv2:maximum 16.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "fslider1_" ;
        lv2:name "feedback";
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "fslider2_" ;
        lv2:name "delay";
        lv2:default 0.1 ;
        lv2:minimum 0.1 ;
        lv2:maximum 5000.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "fslider3_" ;
        lv2:name "level";
        lv2:default -60.0 ;
        lv2:minimum -60.0 ;
        lv2:maximum 0.0 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 4 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "in" ;
        lv2:name "in"
    ] .

<http://guitarix.sourceforge.net/plugins/gxtubedelay#gui>
  a guiext:GtkUI;
  guiext:binary <gxtubedelay_gui.so>;
  guiext:requiredFeature guiext:makeResident .
