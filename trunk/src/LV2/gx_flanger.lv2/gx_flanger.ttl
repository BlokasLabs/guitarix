@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix mod: <http://portalmod.com/ns/modgui#> .

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_flanger>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_flanger" .

<http://guitarix.sourceforge.net/plugins/gx_flanger#_flanger>
    a lv2:Plugin ,
        lv2:EffectPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxFlanger";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_flanger> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    
  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_flanger#gui>;
  
    lv2:minorVersion 28;
    lv2:microVersion 2;

    lv2:port  [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "depth" ;
        lv2:name "DEPTH";
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 5.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "width" ;
        lv2:name "WIDTH";
        lv2:default 5.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "freq" ;
        lv2:name "FREQ";
        lv2:default 0.2 ;
        lv2:minimum 0.05 ;
        lv2:maximum 10.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "feedback" ;
        lv2:name "FEEDBACK";
        lv2:default -0.707 ;
        lv2:minimum -0.99 ;
        lv2:maximum 0.99 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "dry_wet" ;
        lv2:name "WET";
        lv2:default 100.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "mix" ;
        lv2:name "MIX";
        lv2:default 0.0 ;
        lv2:minimum -1.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 6 ;
        lv2:symbol "out" ;
        lv2:name "Out" ;
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 7 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] ;
    mod:gui [ 
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/gx_flanger.html>;
        mod:templateData <modgui/gx_flanger.json>;
        mod:screenshot <modgui/gx_flanger.png>;
        mod:thumbnail <modgui/gx_flanger-thumb.png>;
    ] .


<http://guitarix.sourceforge.net/plugins/gx_flanger#gui>
  a guiext:GtkUI;
  guiext:binary <gx_flanger_gui.so>;
  guiext:requiredFeature guiext:makeResident .
  
