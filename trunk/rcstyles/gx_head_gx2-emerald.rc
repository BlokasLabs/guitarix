style "gx2_b_style"
{
engine "pixmap"
{
image
{
function = BOX
recolorable = TRUE
state = NORMAL
shadow = OUT
file = "gx_green2.jpg"
border = { 0, 0, 0, 0 }
stretch = TRUE
}
image
{
function = BOX
recolorable = TRUE
state = PRELIGHT
shadow = OUT
file = "gx_green1.jpg"
border = { 0, 0, 0, 0 }
stretch = TRUE
}
image
{
function = BOX
recolorable = TRUE
state = ACTIVE
shadow = OUT
file = "gx_green2.jpg"
border = { 0, 0, 0, 0 }
stretch = TRUE
}
}
}

