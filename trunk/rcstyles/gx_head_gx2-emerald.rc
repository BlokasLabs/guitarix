text[ACTIVE] = { 0.70, 0.70, 0.70 }
text[PRELIGHT] = { 0.90, 0.90, 0.90 }
text[INSENSITIVE] = { 0.80, 0.80, 0.80}
text[SELECTED] = { 1.0, 1.0, 1.0 }
#background midi-controller-module
base[ACTIVE] = { 0.90, 0.10, 0.10 }
